{
  "name": "alttp.io",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node node_modules/nodemon/bin/nodemon.js -e ts,js -w ./src/* -i ./src/public -i ./node_modules --exec node -- node_modules/ts-node/dist/bin.js ./src/main.ts",
    "sprites_from_maps": "node node_modules/nodemon/bin/nodemon.js -w src/spriteCreator.ts -e ts --exec node -- node_modules/ts-node/dist/bin.js ./src/spriteCreator.ts -m cvtmaps -i ./src/public/maps -o /src/public/tiles",
    "sprites_to_spr": "node node_modules/nodemon/bin/nodemon.js -w src/spriteCreator.ts -e ts --exec node -- node_modules/ts-node/dist/bin.js ./src/spriteCreator.ts -m cvtsprites -i ./src/public/sprites_src -o ./src/public/spr",
    "start:xnes": "node node_modules/nodemon/bin/nodemon.js -e ts,js -i node_modules -- node_modules/ts-node/dist/bin.js xnesRunner.ts",
    "electron": "electron .",
    "sandbox": "node node_modules/nodemon/bin/nodemon.js -w ./src/sandbox.ts --exec node -- node_modules/ts-node/dist/bin.js ./src/sandbox.ts",
    "start:electron": "npm run build && npm run electron",
    "electron-rebuild": "electron-rebuild -w opencv4nodejs",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.6",
    "@types/pngjs": "^3.3.2",
    "@types/webpack": "^4.4.27",
    "awesome-typescript-loader": "^5.2.1",
    "electron": "4.1.4",
    "electron-rebuild": "^1.8.4",
    "electron-reload": "^1.4.0",
    "native-ext-loader": "^2.3.0",
    "nodemon": "^1.18.11",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-clean-code": "^0.2.9",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "^3.4.4",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "ffmpeg": "0.0.4",
    "ffmpeg-extract-frames": "^2.0.2",
    "ffmpeg.js": "^3.1.9001",
    "file-url": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "import-scripts": "^1.0.0",
    "jpeg-js": "^0.3.5",
    "md5": "^2.2.1",
    "minimist": "^1.2.0",
    "node-video-lib": "^2.1.8",
    "opencv4nodejs": "^4.16.0",
    "pngjs": "^3.4.0"
  }
}